CREATE TABLE engineering (
    project_id INT AUTO_INCREMENT PRIMARY KEY,
    project_name VARCHAR(100) NOT NULL,
    project_type VARCHAR(50) NOT NULL,
    construction_year VARCHAR(25) NOT NULL,
    location VARCHAR(100) NOT NULL,
    description TEXT
);

